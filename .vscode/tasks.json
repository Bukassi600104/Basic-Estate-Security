		{
			"label": "amplify-redeploy-main",
			"type": "shell",
			"command": "aws",
			"args": [
				"amplify",
				"start-job",
				"--app-id",
				"d18ktaplzyr50v",
				"--branch-name",
				"main",
				"--job-type",
				"RELEASE",
				"--region",
				"eu-north-1",
				"--profile",
